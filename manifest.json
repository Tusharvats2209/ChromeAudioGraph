{
  "manifest_version": 2,
  "name": "ext1",
  "version": "0.1",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "js": ["peerjs.min.js","script.js"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Open a background tab every x time"
  }
}